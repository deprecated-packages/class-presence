{
    "name": "migrify/config-class-presence",
    "description": "Check NEON/YAML configs for existing classes",
    "license": "MIT",
    "bin": "bin/config-class-presence",
    "require": {
        "php": "^7.2",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/console": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symfony/yaml": "^4.4|^5.0",
        "nette/utils": "^3.1",
        "nette/neon": "^3.0",
        "symplify/package-builder": "^8.0",
        "symplify/autowire-array-parameter": "^8.0",
        "symplify/smart-file-system": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\ConfigClassPresence\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\ConfigClassPresence\\Tests\\": "tests"
        }
    }
}
