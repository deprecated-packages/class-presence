{
    "name": "symplify/class-presence",
    "description": "Check NEON/YAML/TWIG/LATTE files for existing and missing classes",
    "license": "MIT",
    "bin": [
        "bin/class-presence"
    ],
    "require": {
        "php": ">=7.3",
        "symfony/dependency-injection": "^5.1",
        "symfony/console": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symfony/yaml": "^4.4|^5.1",
        "nette/utils": "^3.0",
        "nette/neon": "^3.2",
        "symplify/symplify-kernel": "^9.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ClassPresence\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ClassPresence\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
